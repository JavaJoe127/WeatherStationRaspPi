@page
@model WeatherStation.Pages.WeatherStats.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>WeatherStats</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WeatherStats.DateStamp)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WeatherStats.DateStamp)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WeatherStats.TempFahrenheit)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WeatherStats.TempFahrenheit)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WeatherStats.TempCelsius)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WeatherStats.TempCelsius)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WeatherStats.Pressure)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WeatherStats.Pressure)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WeatherStats.Humidity)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WeatherStats.Humidity)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.WeatherStats.Id">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>
